<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TeachingEntity" table="teaching" schema="databaseexp">
        <composite-id>
            <key-property name="openTerm" column="openTerm"/>
            <key-property name="cid" column="cid"/>
            <key-property name="tid" column="tid"/>
        </composite-id>
        <property name="ttime" column="ttime"/>
    </class>
</hibernate-mapping>