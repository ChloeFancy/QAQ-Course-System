<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.AdministratorEntity" table="administrator" schema="databaseexp">
        <id name="aid">
            <column name="aid" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="aPassword">
            <column name="aPassword" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="aName">
            <column name="aName" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>