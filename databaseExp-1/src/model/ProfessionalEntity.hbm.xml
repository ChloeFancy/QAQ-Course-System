<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.ProfessionalEntity" table="professional" schema="databaseexp">
        <id name="pid">
            <column name="pid" sql-type="varchar(7)" length="7"/>
        </id>
        <property name="pName">
            <column name="pName" sql-type="char(20)" length="20"/>
        </property>
        <property name="did">
            <column name="did" sql-type="char(2)" length="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>