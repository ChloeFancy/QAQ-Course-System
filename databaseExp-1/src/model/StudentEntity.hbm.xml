<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.StudentEntity" table="student" schema="databaseexp">
        <id name="sid" column="sid"/>
        <property name="sPassword" column="sPassword"/>
        <property name="sName" column="sName"/>
        <property name="sClassName" column="sClassName"/>
        <property name="sSex" column="sSex"/>
        <property name="sBrithday" column="sBrithday"/>
        <property name="pid" column="pid"/>
    </class>
</hibernate-mapping>