<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.BanjiEntity" table="banji" schema="databaseexp">
        <id name="cName">
            <column name="cName" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="cNumber">
            <column name="cNumber" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="pid">
            <column name="pid" sql-type="varchar(7)" length="7" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>